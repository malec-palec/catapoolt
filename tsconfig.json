{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "types": ["vite/client"],
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "target": "ES2020",
    "skipLibCheck": true,
    "outDir": "./build/"
  },
  "include": ["./src/**/*"]
}
